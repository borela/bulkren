{
  "name": "bulkren",
  "version": "2.4.0",
  "description": "Bulk rename utility.",
  "license": "Apache-2.0",
  "bin": {
    "bren": "build/index.js",
    "bulkren": "build/index.js"
  },
  "scripts": {
    "check": "flow check || exit 0",
    "clean": "rimraf \"build/!(.git)\"",
    "build": "npm run clean && run-p build:*",
    "build:target": "webpack --progress --display-error-details",
    "dev": "npm run clean && run-p dev:*",
    "dev:target": "webpack --progress --watch --display-error-details",
    "dist": "npm run clean && run-p dist:*",
    "dist:target": "cross-env NODE_ENV=production webpack --progress --display-error-details",
    "start": "npm run dev"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borela/bulkren.git"
  },
  "bugs": {
    "url": "https://github.com/borela/bulkren/issues"
  },
  "homepage": "https://github.com/borela/bulkren#readme",
  "keywords": [
    "bren",
    "bulk",
    "rename",
    "cli",
    "regex"
  ],
  "author": [
    {
      "name": "Alexandre Borela",
      "email": "alexandre@borela.tech",
      "url": "http://www.borela.tech"
    }
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "diff": "^3.2.0",
    "ls-async": "^2.1.0",
    "sort-array": "^1.1.1",
    "str-to-regexp": "^1.0.0",
    "yargonaut": "^1.1.2",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "ctrine-webpack-settings-cli": "^2.0.1"
  }
}
